class vec2f
{
public:
	x, y: f32;

	func vec3f(x: f32, y: f32)
	{
		this.x = x;
		this.y = y;
	}

	func mul(number: f32): vec3f
	{
		this.x *= number;
		this.y *= number;

		return this;
	}
}

